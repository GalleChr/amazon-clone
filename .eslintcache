ge = (event) => {\r\n    setDisabled(event.empty);\r\n    setError(event.error ? event.error.message : \"\");\r\n  };\r\n\r\n  return (\r\n    <div className=\"payment\">\r\n      <div className=\"payment_container\">\r\n        <h1>\r\n          Checkout (<Link to=\"/checkout\">{basket?.length} items</Link>)\r\n        </h1>\r\n\r\n        <div className=\"payment_section\">\r\n          <div className=\"payment_title\">\r\n            <h3>Delivery Address</h3>\r\n          </div>\r\n          <div className=\"payment_address\">\r\n            <p>{user?.email}</p>\r\n            <p>123 React</p>\r\n            <p>CABA, Bs As</p>\r\n          </div>\r\n        </div>\r\n        <div className=\"payment_section\">\r\n          <div className=\"payment_title\">\r\n            <h3>Review items and delivery</h3>\r\n          </div>\r\n          <div className=\"payment_items\">\r\n            {basket.map((item) => (\r\n              <CheckoutProduct\r\n                id={item.id}\r\n                title={item.title}\r\n                image={item.image}\r\n                price={item.price}\r\n                rating={item.rating}\r\n              />\r\n            ))}\r\n          </div>\r\n        </div>\r\n        <div className=\"payment_section\">\r\n          <div className=\"payment_title\">\r\n            <h3>Payment Method</h3>\r\n          </div>\r\n          <div className=\"payment_details\">\r\n            {/*Stripe magic */}\r\n\r\n            <form onSubmit={handleSubmit}>\r\n              <CardElement onChange={handleChange} />\r\n\r\n              <div className=\"payment_priceContainer\">\r\n                <CurrencyFormat\r\n                  renderText={(value) => <h3>Order Total: {value}</h3>}\r\n                  decimalScale={2}\r\n                  value={getBasketTotal(basket)}\r\n                  displayType={\"text\"}\r\n                  thousandSeparator={true}\r\n                  prefix={\"$\"}\r\n                />\r\n                <button disabled={processing || disabled || succeeded}>\r\n                  <span>{processing ? <p>Processing</p> : \"Buy Now\"}</span>\r\n                </button>\r\n              </div>\r\n              {error && <div>{error}</div>}\r\n            </form>\r\n          </div>\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n}\r\n\r\nexport default Payment;\r\n","D:\\ReactProjects\\amazon-clone\\src\\axios.js",[],{"ruleId":"77","severity":1,"message":"78","line":19,"column":10,"nodeType":"79","messageId":"80","endLine":19,"endColumn":12},{"ruleId":"81","severity":1,"message":"82","line":42,"column":6,"nodeType":"83","endLine":42,"endColumn":8,"suggestions":"84"},{"ruleId":"85","severity":1,"message":"86","line":10,"column":28,"nodeType":"87","messageId":"88","endLine":10,"endColumn":36},{"ruleId":"89","severity":1,"message":"90","line":21,"column":9,"nodeType":"91","endLine":24,"endColumn":11},{"ruleId":"85","severity":1,"message":"92","line":6,"column":12,"nodeType":"87","messageId":"88","endLine":6,"endColumn":18},{"ruleId":"85","severity":1,"message":"86","line":8,"column":28,"nodeType":"87","messageId":"88","endLine":8,"endColumn":36},{"ruleId":"85","severity":1,"message":"93","line":4,"column":10,"nodeType":"87","messageId":"88","endLine":4,"endColumn":24},{"ruleId":"85","severity":1,"message":"86","line":11,"column":22,"nodeType":"87","messageId":"88","endLine":11,"endColumn":30},{"ruleId":"85","severity":1,"message":"92","line":6,"column":12,"nodeType":"87","messageId":"88","endLine":6,"endColumn":18},{"ruleId":"85","severity":1,"message":"86","line":12,"column":28,"nodeType":"87","messageId":"88","endLine":12,"endColumn":36},{"ruleId":"85","severity":1,"message":"94","line":41,"column":11,"nodeType":"87","messageId":"88","endLine":41,"endColumn":18},"no-empty-pattern","Unexpected empty object pattern.","ObjectPattern","unexpected","react-hooks/exhaustive-deps","React Hook useEffect has a missing dependency: 'dispatch'. Either include it or remove t